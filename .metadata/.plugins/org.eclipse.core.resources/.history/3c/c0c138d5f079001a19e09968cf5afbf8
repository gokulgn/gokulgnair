#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int age;
	char name,month,weekdays;

printf("enter your name:");
scanf("%s",&name);
printf("enter your age :");
scanf("%d", &age);
if(age>=18)
{
	printf("you are eligible for benefits");
}
else
{
printf("sorry,you are not eligible for benefits ");
}
printf("\nplease enter the month you born?");
scanf("%s",month);
if ("month==january || february")
{
printf("you can apply on mondays",weekdays);
}
else if ("month==october|| november")
	{
		printf("you can apply on thursdsays",weekdays);
}

	return 0;
}
