#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int age;
	char name;

printf("enter your name:");
scanf("%s",&name);
printf("enter your age :");
scanf("%d", &age);
if(age>=18)
{
	printf("you are eligible to vote");
}
else
{
printf("sorry,you are not eligible to vote");
}
	return 0;
}
